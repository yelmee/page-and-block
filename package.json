{
  "name": "page-and-block",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "starts": "npm run db-login update-types db-local",
    "lint": "eslint .",
    "update-types": "supabase gen types --lang=typescript --project-id arfycurykubypljntdmm > database.types.ts",
    "type-check": "tsc --noEmit",
    "db-login": "supabase login",
    "db-local": "supabase init && supabase start",
    "db-migr": "supabase migration",
    "db-push": "supabase db push",
    "db-type": "supabase gen type",
    "db-backup": "supabase db diff -f my_schema && supabase db dump --local --data-only > supabase/seed.sql && supabase stop --no-backup",
    "db-link": "supabase link --project-ref arfycurykubypljntdmm",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
